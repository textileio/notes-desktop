    "animate.css": "^3.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.1.1",
    "electron-reload": "^1.4.0",
    "extract-css-chunks-webpack-plugin": "^4.3.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typeface-heebo": "^0.0.72",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-log": "^2.0.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-null-plugin": "^0.0.2"
  },
  "build": {
    "productName": "Notes",
    "appId": "io.textile.notes",
    "artifactName": "Notes-${version}.${ext}",
    "compression": "normal",
    "directories": {
      "output": "release/"
    },
    "files": [
      "build/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "assets/icon.icns",
      "type": "distribution",
      "target": [
        "pkg",
        "dmg"
      ]
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": "nsis"
    },
    "linux": {
      "category": "Network",
      "icon": "assets/icon.png",
      "target": [
        "AppImage"
      ]
    }
  }
}
