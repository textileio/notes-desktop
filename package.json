{
  "name": "@textile/notes-desktop",
  "version": "0.0.1",
  "description": "Simple React.js UI to sync with Textile Notes mobile app",
  "main": "build/main.js",
  "scripts": {
    "start": "webpack-dev-server  --mode development --inline --hot --open",
    "build:web": "webpack  --mode development",
    "lint": "./node_modules/tslint/bin/tslint  -c tslint.json --project .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:electron": "webpack --config webpack.electron.ts --progress --mode=development",
    "clean:electron": "rimraf build:electron",
    "rebuild:electron": "npm-run-all clean:electron build:electron",
    "prerelease:electron": "webpack --config webpack.electron.ts --progress --mode=production",
    "build": "build"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@textile/js-http-client": "0.2.18",
    "actuatejs": "^1.0.2",
    "electron-is-dev": "^1.1.0",
    "electron-pug": "^2.0.0",
    "electron-store": "^3.2.0",
    "electron-window-state": "^5.0.3",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "qrcode.react": "^0.9.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-emotion": "^10.0.0",
    "react-modal": "^3.8.1",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-electron-storage": "^2.1.0",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "slate": "^0.45.1",
    "slate-auto-exit-block": "^1.1.0",
    "slate-react": "^0.21.21",
    "ts-md5": "^1.2.4",
    "tslib": "^1.9.3",
    "typesafe-actions": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@textile/tslint-rules": "^1.0.3",
    "@types/copy-webpack-plugin": "^4.4.3",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-store": "^1.3.1",
    "@types/electron-window-state": "^2.0.33",
    "@types/eslint": "^4.16.6",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node-sass": "^4.11.0",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-modal": "^3.8.2",
    "@types/react-redux": "^7.0.8",
    "@types/react-router-dom": "^4.3.2",
    "@types/react-transition-group": "^2.9.0",
    "@types/redux-persist": "^4.3.1",
    "@types/rimraf": "^2.0.2",
    "@types/slate": "^0.44.3",
    "@types/slate-react": "^0.21.2",
    "@types/systeminformation": "^3.23.1",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "^4.4.27",
    "@types/webpack-env": "^1.13.9",
    "@types/webpack-merge": "^4.1.3",
